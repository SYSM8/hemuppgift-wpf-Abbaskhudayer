<Window x:Class="Hemuppgift_WPFtemplate.DockPanelWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hemuppgift_WPFtemplate"
        mc:Ignorable="d"
        Title="DockPanelWindow" Height="450" Width="800">
    <Grid>
        <DockPanel>
            <Button DockPanel.Dock="Top" x:Name="btntop" Content="Top" Width="80" Height="30" Margin="5"/>
            <Button DockPanel.Dock="Left" x:Name="btnleft" Content="Left" Width="80" Height="30" Margin="5"/>
            <Button DockPanel.Dock="Right" x:Name="btnright" Content="Right" Width="80" Height="30" Margin="5"/>
            <Button DockPanel.Dock="Bottom" x:Name="btnbottom" Content="Bottom" Width="80" Height="30" Margin="5"/>
            <Button x:Name="btncenter" Content="Center" Width="80" Height="30" Margin="5" Click="btncenter_Click"/>
        </DockPanel>

        <Image x:Name="Pokal" Source="C:\Users\abbas\OneDrive\Dokument\Programmering C#\WpfGenomgang\WpfHemuppgift\Hemuppgift_WPFtemplate\TrophyImage.jpg" Height="150" Width="150" Visibility="Hidden"/>


    </Grid>
</Window>
